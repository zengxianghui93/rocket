/* pages/mtydream/index.wxss */
page{
    background-color: #E7F1FC;
}
.tadream_add{
    position: fixed;
    right: 35rpx;
    top: 1050rpx;
    width: 106rpx;
    height: 106rpx;
    background: url('data:image/png;base64,');
    background-size: 100%;
    background-repeat: no-repeat;
    border-radius: 50%;
}
.tadream_main_container{
    margin: 24rpx auto;
    width: 683rpx;
    height: 98rpx;
    border-radius: 20rpx 20rpx 20rpx 20rpx;
    background-color: rgba(255, 255, 255, 100);
    text-align: left;
    margin-bottom: 4rpx;
    box-sizing: border-box;
    overflow: hidden;
    display: flex;
    align-items: center;
    background: url('data:image/png;base64,');
    background-size: 100%;
    background-repeat: no-repeat;

}
.tadream_main_container_size{
    padding-left: 84rpx;
    color: #1485EE;
    font-size: 18px;
    text-align: left;
    font-family: PingFangSC-regular;
}
.tadream-nomain{
    position: relative;
    top: 140rpx;
    left: 60rpx;
    width: 315rpx;
    height: 403rpx;
    color: rgba(16, 16, 16, 100);
    font-size: 20px;
    text-align: center;
    font-family: PingFangSC-regular;
}
.tadream-nomain image:nth-child(1) {
    position: absolute;
    width: 630rpx;
    height: 806rpx;
    left: 0;
    top: 0;
}
.tadream-nomain image:nth-child(2) {
    position: absolute;
    left: 88rpx;
    top: 96rpx;
    width: 436rpx;
    height: 206rpx;
}
.arrow{
    border: 4rpx solid rgba(3, 127, 249, 100);
}
.arrow_box{
    animation: glow 600ms ease-out infinite alternate;
}
@keyframes glow {
    0% {
        opacity: 1;
        border-color: rgba(3, 127, 249, 100);
        box-shadow: 0 4rpx 12rpx 0px rgba(3, 127, 249, 100);
    }
    100% {
        opacity: .5;
        border-color: rgb(111 184 255);
        box-shadow: 0px 4rpx 12rpx 0px rgb(111 184 255);
    }
}
.notdata{
    margin: 160rpx auto;
    width: 590rpx;
    height: 896rpx;
    position: relative;
}
.notdata image{
    width: inherit;
    height: inherit;
}
.notdata view {
    position: absolute;
    width: 278rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 18px;
    color: #FFFFFF;
    bottom: 175rpx;
    left: 160rpx;
    font-weight: 500;
    text-align: center;
}