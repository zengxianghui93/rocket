/* pages/mtydream/index.wxss */
page{
    background-color: #E7F1FC;
}
.mydream{
    padding-bottom: 200px;
    box-sizing: border-box;
}
.mydream_add{
    position: fixed;
    right: 35rpx;
    top: 1050rpx;
    width: 106rpx;
    height: 106rpx;
    background: url('data:image/png;base64,');
    background-size: 100%;
    background-repeat: no-repeat;
    border-radius: 50%;
}
.mydream_main_container{
    margin: 24rpx auto;
    width: 683rpx;
    height: 98rpx;
    border-radius: 20rpx 20rpx 20rpx 20rpx;
    margin-bottom: 4rpx;
    overflow: hidden;
    display: flex;
    align-items: center;
    box-sizing: border-box;
    background: url('data:image/png;base64,');
    background-size: 100%;
    background-repeat: no-repeat;
}

.mydream_main_container_size{
    width: 683rpx;
    transform: translateX(172rpx);
    margin-left: -172rpx;
    transition: all 0.4s;
    padding-left: 34rpx;
    color: #1485EE;
    font-size: 18px;
    font-family: PingFangSC-regular;
    z-index: 2;
    padding-left: 84rpx;
    height: 98rpx;
    line-height: 98rpx;
}

.complete_btn {
    border-radius: 20rpx 20rpx 20rpx 20rpx;
    width: 172rpx;
    text-align: center;
    background: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 100%;
    line-height: 100rpx;
    color: #fff;
    transition: all 0.4s;
    transform: translateX(172rpx);
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 999;
}

.complete_btn view:nth-child(1){
    width: 50rpx;
    height: 50rpx;
}
.complete_btn view:nth-child(2){
    color: rgba(255, 255, 255, 100);
    font-size: 18px;
    font-family: PingFangSC-regular;
}
.slide .complete_btn {
   transform: translateX(0);
}
.notdata{
    margin: 160rpx auto;
    width: 590rpx;
    height: 896rpx;
    position: relative;
}
.notdata image{
    width: inherit;
    height: inherit;
}
.notdata view {
    position: absolute;
    width: 230rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 18px;
    color: #FFFFFF;
    bottom: 175rpx;
    left: 200rpx;
    font-weight: 500;
}