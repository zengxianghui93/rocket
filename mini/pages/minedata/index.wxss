/* pages/minedata/index.wxss */
page{
    background-color: #E7F1FC;
}
.mine_title{
    margin: 124rpx;
    width: 502rpx;
    height: 442rpx;
    background: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 100%;
}
.mine_title_portrait{
    display: flex;
    justify-content: space-between;
    width: 502rpx;
    padding-top: 100rpx;
   
}
.mine_title_portrait-top{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.mine_title_portrait-top image{
    width: 122rpx;
    height: 122rpx;
    border-radius: 50%;
}
.mine_title_portrait-top view{
    margin-top: 24rpx;
    color: #1485EE;
    font-size: 19px;
    font-weight: 500;
}
.mine_title_button{
    margin-top: 55rpx;
    margin-left: 80rpx;
    font-size: 19px;
    color: #1485EE;
    font-weight: 500;
}
.mine_main{
    display: flex;
    width: 642rpx;
    justify-content: space-between;
    margin: auto;
}
.mine_main_cont{
    width: 304rpx;
    height: 374rpx;
    background-color: #FFFFFF;
    border-radius: 24rpx;
}
.mine_main_cont_top{
    font-size: 17px;
    color: #1485EE;
    margin-top: 50rpx;
    margin-left: 42rpx;
}
.mine_main_cont_left{
    width: 118rpx;
    height: 150rpx;
    margin-left: 150rpx;
    margin-top: 80rpx;
    background: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 100%;
}
.mine_main_cont_right{
    width: 118rpx;
    height: 150rpx;
    margin-left: 120rpx;
    margin-top: 80rpx;
    background: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 100%;
}
.add_Mask{
    position: fixed ;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    background-color: rgba(0,0,0,0.47);
    z-index: 999;
}
.add_Mask_content{
    left: 12.5%;
    top: 35%;
    position: absolute;
    width: 560rpx;
    height: 330rpx;
    line-height: 40rpx;
    z-index: 1000;
    border-radius: 8rpx;
    background-color: #FFFFFF;
    text-align: center;
}
.add_Mask_content_top{
    color: rgba(0, 0, 0, 1);
    font-size: 18px;
    text-align: center;
    font-family: PingFangSC-regular;
    margin-top: 50rpx;
}
.add_Mask_content_cont{
    margin: 30rpx auto;
    width: 458rpx;
    height: 88rpx;
    color: rgba(136, 136, 136, 100);
    font-size: 15px;
    text-align: left;
    font-family: PingFangSC-regular;
}
.add_Mask_content_button{
    margin: 20rpx auto;
    display: flex;
    justify-content: space-between;
    width: 400rpx;
    font-size: 18px;
    font-family: PingFangSC-regular;
}
.add_Mask_content_button_config{
    color: rgba(0, 0, 0, 1);
}
.add_Mask_content_button_cancel{
    color: #1485EE;
}